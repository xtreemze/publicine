{
  "name": "publicine",
  "version": "1.0.0",
  "description": "Cine",
  "main": "index.html",
  "scripts": {
    "build": "node ./rmDir.js && webpack --env=prod --profile",
    "dev": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile --open",
    "hotDebug": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile",
    "debug": "webpack-dev-server --host localhost --port 8080 --env=dev --profile",
    "net2": "webpack-dev-server --host 192.168.0.130 --port 80 --env=dev --profile --open",
    "watch": "webpack --progress --colors --env=dev --profile --watch",
    "exportp": "node ./rmDir.js && webpack --env=prod --profile --progress",
    "rm": "node ./rmDir.js",
    "clean": "node ./clean.js && npm i -D && npm up -D"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/publicine.git"
  },
  "keywords": [
    "cine",
    "progressive",
    "webapp",
    "materialize",
    "JavaScript"
  ],
  "author": "Carlos Velasco",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/publicine/issues"
  },
  "homepage": "https://github.com/xtreemze/publicine#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "browserslist": "^2.11.3",
    "css-loader": "^0.28.11",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-minifier-webpack-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.6.0",
    "jquery": "^3.3.1",
    "materialize-css": "^0.100.2",
    "offline-plugin": "^4.9.1",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  }
